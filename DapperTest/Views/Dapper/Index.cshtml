
@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>
<form method="post" action="/Dapper/Create">
    <input type="text" name="Email" />
    <input type="password" name="Password" />
    <input type="submit" name="Create" />
</form>

<form action="/Dapper/Update">
    <input type="text" name="Email" />
    <input type="password" name="Password" />
    <input type="submit" name="Update" />
</form>
<br />
<!--Button trigger modal-->
<button id="clientButton" type="button" class="btn btn-primary" data-toggle="modal">
    Launch All Clients
</button>
<!--Modal-->
<div class="modal fade" id="exampleModel" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Modal Title</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <p id="ProductDescription"></p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-success" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary">Save changes</button>
            </div>
        </div>
    </div>
</div>

<script src="~/js/site.min.js"></script>
<script src="~/lib/signalr/dist/browser/signalr.min.js"></script>
<script src="~/js/JavaScript.js"></script>